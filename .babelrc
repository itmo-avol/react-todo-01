{
	"presets": [
		[
			"@babel/preset-env",
			{
				"bugfixes": true,
				"modules": false
			}
		],
		[
			"@babel/preset-typescript",
			{
				"allowDeclareFields": true,
				"onlyRemoveTypeImports": true
			}
		],
		[
			"@babel/preset-react",
			{
				"runtime": "automatic"
			}
		]
	],
	"plugins": [
		"@babel/proposal-class-properties"
	]
}
